#include <iostream>
#include <string>

using namespace std;
struct student {
    string name;
    string surroname;
    int index;
};
student dane[10]={
    {"Anna", "Kowalska", 7},
{"Jan", "Nowak", 23},
{"Katarzyna", "Wiśniewska", 5},
{"Piotr", "Kamiński", 12},
{"Maria", "Lewandowska", 8},
{"Tomasz", "Wójcik", 19},
{"Joanna", "Zielińska", 3},
{"Michał", "Szymański", 14},
{"Agnieszka", "Dąbrowska", 9},
{"Adam", "Kozłowski", 17}
};

int main() {
    int tab[10];
    int s = 0;

    for (int i = 0; i < 10; i++) {
        cout << "Podaj ocene "<<  dane[i].name << " " << dane[i].surroname  << endl;
        cin >>tab[i];
    }
    cout << endl;
   for (int i = 0; i < 10; i++) {
       cout << "ocena " << dane[i].index << " " << tab[i] << endl;
   }
    cout << endl;

    cout << endl;

    return 0;
}
