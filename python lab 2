#ZAD6
przedmioty = {
    "Matematyka": 90,
    "Fizyka": 85,
    "Biologia": 78
}
for przedmiot, punkty in przedmioty.items():
    print(f"Przedmiot: {przedmiot}, Punkty: {punkty}")
